{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "commonjs",
    "alwaysStrict": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "target": "ES2017",
    "strictNullChecks": true,
    "outDir": "build",
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/*.ts", "src/*/*.ts"],
  "exclude": []
}
